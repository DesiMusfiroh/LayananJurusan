            android:id="@+id/tv_perjanjian_kerja"
            android:layout_width="200dp"
            android:padding="10sp"
            android:text="Perjanjian Kerja"
            style="@style/table_cell_body"/>
        <TextView
            android:id="@+id/tv_ijin_usaha"
            android:layout_width="200dp"
            android:padding="10sp"
            android:text="Ijin Usaha"
            style="@style/table_cell_body"/>
