            android:id="@+id/tv_kriteria_kewiraswastaan"
            android:layout_width="200dp"
            android:padding="10sp"
            android:text="Kriteria Kewiraswastaan"
            style="@style/table_cell_body"/>

        <TextView
            android:id="@+id/tv_kriteria_usaha"
            android:layout_width="200dp"
            android:padding="10sp"
            android:text="Kriteria Usaha"
            style="@style/table_cell_body"/>

        <TextView
            android:id="@+id/tv_nama_usaha"
            android:layout_width="200dp"
            android:padding="10sp"
            android:text="Nama Usaha"
            style="@style/table_cell_body"/>
        <TextView
            android:id="@+id/tv_ijin_usaha_wiraswasta"
            android:layout_width="200dp"
            android:padding="10sp"
            android:text="Ijin Usaha"
            style="@style/table_cell_body"/>
        <TextView
            android:id="@+id/tv_masa_tunggu_lanjutan_studi"
            android:layout_width="150dp"
            android:padding="10sp"
            android:text="Masa Tunggu"
            style="@style/table_cell_body"/>
        <TextView
            android:id="@+id/tv_jenjang_prodi_lanjutan"
            android:layout_width="200dp"
            android:padding="10sp"
            android:text="Jenjang Prodi Lanjutan"
            style="@style/table_cell_body"/>
        <TextView
            android:id="@+id/tv_nama_prodi_lanjutan"
            android:layout_width="200dp"
            android:padding="10sp"
            android:text="Nama Prodi"
            style="@style/table_cell_body"/>
        <TextView
            android:id="@+id/tv_perguruan_tinggi"
            android:layout_width="250dp"
            android:padding="10sp"
            android:text="Nama Perguruan Tinggi"
            style="@style/table_cell_body"/>

        <TextView
            android:layout_width="150dp"
            android:padding="10sp"
            android:text="Bukti"
            style="@style/table_cell_body"/>
</TableRow>