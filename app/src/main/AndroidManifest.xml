<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.layanan.jurusan">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Jurusan"
        android:usesCleartextTraffic="true">
        <activity android:name=".ui.notification.NotificationActivity"></activity>
        <activity android:name=".ui.profile.SignatureActivity" />
        <activity android:name=".ui.iku.Iku1Activity" />
        <activity android:name=".ui.prodi.ProdiActivity" />
        <activity android:name=".ui.announcement.ListAnnouncementActivity" />
        <activity android:name=".ui.announcement.AnnouncementActivity" />
        <activity android:name=".ui.jurusan.JurusanActivity" />
        <activity android:name=".ui.CharacterActivity" />
        <activity android:name=".ui.news.TestNewsActivity" />
        <activity
            android:name=".ui.news.ListNewsActivity"
            android:parentActivityName=".ui.student.DashboardStudentActivity" />
        <activity android:name=".ui.news.NewsActivity" />
        <activity android:name=".ui.profile.ProfileActivity" />
        <activity android:name=".ui.student.DashboardStudentActivity">

            <!-- <intent-filter> -->
            <!-- <action android:name="android.intent.action.MAIN" /> -->


            <!-- <category android:name="android.intent.category.LAUNCHER" /> -->
            <!-- </intent-filter> -->
        </activity>
        <activity android:name=".ui.login.LoginActivity" />
        <activity android:name=".SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <service android:name=".FcmServices">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>